    <title>Circlelink</title>
    <%= csrf_meta_tags %>
    <script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
    <script src="http://code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>

       <div id="header">
         <div><%=link_to (image_tag "/images/circle_log.png",{:size => "250x72",:class=>"logo_space"}),homes_path%></div>
       </div>
       <div id="menu">
           <ul>
             <li><%=link_to "　HOME　",homes_path,{:style=>"text-decoration:none;"}%></li>
             <li><%=link_to "新着情報",homes_index_show_info_path,{:style=>"text-decoration:none;"}%></li>
             <li><%=link_to "サークル・部活一覧",homes_circle_path,{:style=>"text-decoration:none;"}%></li>
             <li><%=link_to "Neoニートとは！？",neets_path,{:style=>"text-decoration:none;"}%></li>
           </ul>
         </div>

    <%= yield %>

    <div id="hutter">
      <ul class="login">
        <li><%=link_to "管理者ログイン",admin_session_path,{:class=>"login_text"}%></li>
      </ul>
      <div><%=link_to (image_tag "/images/circle_log.png",{:size => "200x60",:class=>"logo_space"}),homes_path%></div>
    </div>
