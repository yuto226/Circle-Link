<div class = "check_button">


<%= form_for :conditions,{:method => 'get'} do |f|%>
<%Profile.all.each do |p|%>
<%= f.check_box :p_ids,{:name => "conditions[p_ids][]"},
p.id.to_s%>
<%=p.id%>
<%end%>

<%= f.submit("行け―！") %>
<% end %>

